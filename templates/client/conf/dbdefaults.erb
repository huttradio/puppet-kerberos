
[dbdefaults]
<% if @database_module -%>
database_module = <%= @database_module %>
<% end -%>
<% if @ldap_kerberos_container_dn -%>
ldap_kerberos_container_dn = <%= @ldap_kerberos_container_dn %>
<% end -%>
<% if @ldap_kdc_dn -%>
ldap_kdc_dn = <%= @ldap_kdc_dn %>
<% end -%>
<% if @ldap_kadmind_dn -%>
ldap_kadmind_dn = <%= @ldap_kadmind_dn %>
<% end -%>
<% if @ldap_service_password_file -%>
ldap_service_password_file = <%= @ldap_service_password_file %>
<% end -%>
<% if @ldap_servers -%>
ldap_servers = <%= Array(@ldap_servers).join(" ") %>
<% end -%>
<% if @ldap_conns_per_server -%>
ldap_conns_per_server = <%= @ldap_conns_per_server %>
<% end -%>
