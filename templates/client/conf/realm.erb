<%= @realm %> = {
<% if @admin_server -%>
	admin_server = <%= @admin_server %>
<% end -%>
<% if @auth_to_local -%>
<% Array(@auth_to_local).each do |value| -%>
	auth_to_local = <%= value %>
<% end -%>
<% end -%>
<% if @auth_to_local_names -%>
	auth_to_local_names = {
<% @auth_to_local_names.each_pair do |key, value| -%>
	<%= key %> = <%= value %>
<% end -%>
	}
<% end -%>
<% if @default_domain -%>
	default_domain = <%= @default_domain %>
<% end -%>
<% if @http_anchors -%>
<% Array(@http_anchors).each do |value| -%>
	http_anchors = <%= value %>
<% end -%>
<% end -%>
<% if @kdc -%>
<% Array(@kdc).each do |value| -%>
	kdc = <%= value %>
<% end -%>
<% end -%>
<% if @kpasswd_server -%>
	kpasswd_server = <%= @kpasswd_server %>
<% end -%>
<% if @master_kdc -%>
	master_kdc = <%= @master_kdc %>
<% end -%>
<% if @v4_instance_convert -%>
	v4_instance_convert = <%= @v4_instance_convert %>
<% end -%>
<% if @v4_realm -%>
	v4_realm = <%= @v4_realm %>
<% end -%>
}
